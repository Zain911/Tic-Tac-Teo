<AnchorPane id="AnchorPane" onKeyPressed="#onPlayerTwoKeyPressed" prefHeight="649.0" prefWidth="1087.0" style="-fx-background-color: #ffffff;" styleClass="bodybg" stylesheets="@../css/Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PlayerNameDialogController">
   <children>
      <TextField fx:id="edtNameTwo" layoutX="598.0" layoutY="427.0" prefHeight="41.0" prefWidth="191.0" promptText="player 2" styleClass="button" stylesheets="@../css/Style.css">
         <font>
            <Font name="Andalus" size="18.0" />
         </font></TextField>
      <Text fx:id="txtError" fill="#f50101" layoutX="481.0" layoutY="452.0" strokeType="OUTSIDE" strokeWidth="0.0" text="please enter players name!" visible="false" wrappingWidth="197.48046875">
         <font>
            <Font size="13.0" />
         </font></Text>
      <TextField fx:id="edtNameOne" layoutX="311.0" layoutY="426.0" onKeyPressed="#onPlayerOneKeyPressed" prefHeight="41.0" prefWidth="178.0" promptText="player 1" styleClass="button" stylesheets="@../css/Style.css">
