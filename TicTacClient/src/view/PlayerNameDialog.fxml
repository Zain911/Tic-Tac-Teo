      <Button fx:id="btnPlay" layoutX="513.0" layoutY="517.0" mnemonicParsing="false" onAction="#startPlay" prefHeight="41.0" prefWidth="104.0" style="-fx-background-color: #0D5F5F;" stylesheets="@../css/Style.css" text="start Game" textFill="WHITE">
